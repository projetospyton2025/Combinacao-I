.resultado-box {
    max-height: 300px;
    overflow-y: auto;
    padding: 15px;
}

.numero {
    display: inline-block;
    padding: 5px 10px;
    margin: 2px;
    background-color: #f8f9fa;
    border: 1px solid #dee2e6;
    border-radius: 4px;
}

.error-message {
    color: #dc3545;
    margin-top: 5px;
    display: none;
}
